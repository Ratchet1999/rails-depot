<div id="<%= dom_id order %>">

  <p>
    <h3><u><strong>Order:</strong><%= order.id %></u></h3>
  </p>

  <p>
    <strong>Name:</strong>
    <%= order.name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= order.address %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= order.email %>
  </p>

  <p>
    <strong>Pay type:</strong>
    <%= order.pay_type %>
  </p>

</div>
